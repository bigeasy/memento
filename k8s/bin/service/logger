#!/bin/bash

source $(dirname $(readlink -f $0))/../functions.bash

[[ ! -z "$ADDENDUM_LOGGER_AUTHORITY" ]] || \
    service_abend "ADDENDUM_LOGGER_AUTHORITY is required to run relay logger."

echo prolific tcp://$ADDENDUM_LOGGER_AUTHORITY prolific.udp --bind 514
prolific tcp://$ADDENDUM_LOGGER_AUTHORITY prolific.udp --bind 514
